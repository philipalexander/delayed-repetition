<%= link_to "New Card", new_card_path(current_user) %>
<h1>Should only be showing the cards you created</h1>
<table>
  <tr>
    <th>front</th>
    <th>back</th>
    <th>id</th>

  </tr>
 
  <% @user_cards.each do |card| %>
    <tr>
      <td><%= card.front %></td>
      <td><%= card.back %></td>
      <td><%= card.user_id %></td>
      
      <td><%= link_to 'Show', card_path(card) %></td>
      <td><%= link_to 'Edit', edit_card_path(card) %></td>
      <td><%= link_to 'Destroy', card_path(card),
              method: :delete,
              data: { confirm: 'Are you sure?' } %></td>
    </tr>
  <% end %>
</table>
